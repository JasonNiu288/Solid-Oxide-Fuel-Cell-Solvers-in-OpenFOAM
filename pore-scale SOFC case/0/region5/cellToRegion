FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/region5";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 6;

boundaryField
{
    bottom
    {
        type            zeroGradient;
    }
    region5_to_YSZ
    {
        type            calculated;
        value           uniform 0;
    }
}


