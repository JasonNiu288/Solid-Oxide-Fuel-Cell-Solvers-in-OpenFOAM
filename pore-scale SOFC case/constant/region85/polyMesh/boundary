FoamFile
{
    version     2.0;
    format      ascii;
    class       polyBoundaryMesh;
    location    "constant/region85/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

3
(
    region85_to_region84
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          1;
        startFace       0;
        sampleMode      nearestPatchFace;
        sampleRegion    region84;
        samplePatch     region84_to_region85;
    }
    region85_to_region6
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          1;
        startFace       1;
        sampleMode      nearestPatchFace;
        sampleRegion    region6;
        samplePatch     region6_to_region85;
    }
    region85_to_Ni
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          4;
        startFace       2;
        sampleMode      nearestPatchFace;
        sampleRegion    Ni;
        samplePatch     Ni_to_region85;
    }
)

