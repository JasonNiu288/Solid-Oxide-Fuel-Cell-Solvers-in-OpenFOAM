FoamFile
{
    version     2.0;
    format      ascii;
    class       polyBoundaryMesh;
    location    "constant/region808/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

3
(
    region808_to_YSZ
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          3;
        startFace       0;
        sampleMode      nearestPatchFace;
        sampleRegion    YSZ;
        samplePatch     YSZ_to_region808;
    }
    region808_to_membrane
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          1;
        startFace       3;
        sampleMode      nearestPatchFace;
        sampleRegion    membrane;
        samplePatch     membrane_to_region808;
    }
    region808_to_region773
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          2;
        startFace       4;
        sampleMode      nearestPatchFace;
        sampleRegion    region773;
        samplePatch     region773_to_region808;
    }
)

