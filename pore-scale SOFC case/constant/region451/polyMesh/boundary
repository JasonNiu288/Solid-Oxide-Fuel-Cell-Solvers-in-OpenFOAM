// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

2
(
    region451_to_YSZ
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          4;
        startFace       0;
        sampleMode      nearestPatchFace;
        sampleRegion    YSZ;
        samplePatch     YSZ_to_region451;
    }
    region451_to_pore
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          2;
        startFace       4;
        sampleMode      nearestPatchFace;
        sampleRegion    pore;
        samplePatch     pore_to_region451;
    }
)

