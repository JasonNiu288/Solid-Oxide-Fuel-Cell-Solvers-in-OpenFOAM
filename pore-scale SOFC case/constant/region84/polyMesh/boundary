FoamFile
{
    version     2.0;
    format      ascii;
    class       polyBoundaryMesh;
    location    "constant/region84/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

2
(
    region84_to_region85
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          1;
        startFace       0;
        sampleMode      nearestPatchFace;
        sampleRegion    region85;
        samplePatch     region85_to_region84;
    }
    region84_to_YSZ
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          5;
        startFace       1;
        sampleMode      nearestPatchFace;
        sampleRegion    YSZ;
        samplePatch     YSZ_to_region84;
    }
)

